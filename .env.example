# Supabase
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Configuration for Chat with Elya (Optional)
# The app works perfectly with simulated responses if no API key is configured

# AI Provider - Options: 'agenticflow', 'openai', 'openrouter', or 'simulated' (default: agenticflow)
VITE_AI_PROVIDER=agenticflow

# Agentic Flow Configuration (default provider)
# Your Elya agent ID from Agentic Flow
VITE_AGENTICFLOW_AGENT_ID=a1cab40c-bcc2-49d8-ab97-f233f9b83fb2
# Optional: Add API key if your Agentic Flow agent requires authentication
# VITE_AGENTICFLOW_API_KEY=

# OpenAI API Key (only needed if provider is 'openai')
# Get your API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=

# OpenRouter API Key (required only if provider is 'openrouter')
# VITE_OPENROUTER_API_KEY=

# AI Model (optional, defaults vary by provider)
# OpenAI: gpt-3.5-turbo, gpt-4, gpt-4-turbo-preview
# OpenRouter: openrouter/sonoma-sky-alpha, etc.
VITE_AI_MODEL=

# Stripe Configuration
# Get your test keys from: https://dashboard.stripe.com/test/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
# Note: The secret key should only be used on the server side (not in frontend)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# Encharge Email Marketing & Automation
# Get your API key from: https://app.encharge.io/settings/api-keys
# Write Key is used for sending data TO Encharge (creating users, tracking events)
VITE_ENCHARGE_WRITE_KEY=your_encharge_write_key_here
# API Key is used for authentication with the Encharge API
VITE_ENCHARGE_API_KEY=your_encharge_api_key_here

# Sentry Error Tracking & Performance Monitoring
# Get your DSN from: https://sentry.io/settings/[your-org]/projects/[your-project]/keys/
# Leave empty to disable Sentry (will log to console instead)
VITE_SENTRY_DSN=
# Optional: Enable Sentry in development for testing (default: disabled in dev)
# VITE_SENTRY_DEBUG=true
