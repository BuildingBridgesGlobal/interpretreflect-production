# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Configuration for Chat with Elya (Optional)
# The app works perfectly with simulated responses if no API key is configured

# AI Provider - Options: 'openai' or 'simulated' (default: simulated)
VITE_AI_PROVIDER=simulated

# OpenAI API Key (required only if provider is 'openai')
# Get your API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=

# AI Model (optional, defaults to gpt-3.5-turbo)
# Options: gpt-3.5-turbo, gpt-4, gpt-4-turbo-preview
VITE_AI_MODEL=gpt-3.5-turbo

# Stripe Configuration
# Get your test keys from: https://dashboard.stripe.com/test/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
# Note: The secret key should only be used on the server side (not in frontend)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
